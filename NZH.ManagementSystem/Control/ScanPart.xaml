<UserControl x:Class="NZH.ManagementSystem.Control.ScanPart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NZH.ManagementSystem.Control"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="550">
    <Grid Name="GridAll" Background="Red">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Label Name="lbERP_PART_NO" Grid.Row="1" Grid.Column="0" Content="" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Label Name="lbPART_NAME" Grid.Row="1" Grid.Column="1" Content="" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Label Name="lbSCAN_BARCODE" Grid.Row="1" Grid.Column="2" Content="" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Line Grid.Row="1" Grid.ColumnSpan="15" X1="0" Y1="30" X2="920" Y2="30" Stroke="Black"  StrokeThickness="1"></Line>
    </Grid>
</UserControl>
